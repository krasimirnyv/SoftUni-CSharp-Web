@{
    ViewData["Title"] = "Products";

    var products = new[]
    {
        new {
            Name = "Logitech K120 Keyboard",
            Url = "https://www.logitech.com/en-us/products/keyboards/k120-usb-standard-computer-keyboard.920-002478.html",
            Image = "https://ardes.bg/uploads/original/logitech-k120-cheren-611760.jpg"
        },
        new {
            Name = "Samsung Galaxy Buds2 Pro",
            Url = "https://www.samsung.com/global/galaxy/galaxy-buds2-pro/",
            Image = "https://cdn.technomarket.bg/ng/240/uploads/library/product/09208932/62ec9eff61f7e.jpg.webp"
        },
        new {
            Name = "Sony WH-1000XM5 Headphones",
            Url = "https://www.sony.com/electronics/headband-headphones/wh-1000xm5",
            Image = "data:image/jpeg;base64,"
        }
    };
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.PageTitle</title>
</head>
<body>

<style>
        ul {
            list-style: none;
            padding: 0;
        }

        img {
            max-width: 200px;
            display: block;
            margin-top: 10px;
        }
    </style>

<h1>@ViewBag.PageTitle</h1>
<p>List with products:</p>

<ul>
    @foreach (var product in products)
    {
        <li>
            <a href="@product.Url" target="_blank">
                @product.Name
            </a>
            <img src="@product.Image" alt="@product.Name"/>
        </li>
    }
</ul>

<p>
    Този текст доказва, че стойността идва от Controller чрез ViewBag.
</p>
</body>
</html>